{
  "version": 2,
  "buildCommand": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps --include=optional && npm run build:vercel",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install --legacy-peer-deps --include=optional",
  "env": {
    "NODE_VERSION": "18.x",
    "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
    "NPM_CONFIG_INCLUDE": "optional"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}